{
    "name": "revelare/ws-boleto-santander",
    "description": "Um fork personalizado de WS Boletos Santander de DenysXavier para facilitar a integração entre aplicativos feitos em PHP e a geração de boletos online no banco Santander.",
    "homepage": "https://github.com/revelare/ws-boleto-santander",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Heitor Figueiredo",
            "email": "heitor@heitorfig.com",
            "homepage": "http://www.heitorfig.net"
        }
    ],
    "support": {
        "issues": "https://github.com/heitorfig/WSBoletoSantander/issues",
        "source": "https://github.com/heitorfig/WSBoletoSantander"
    },
    "require": {
        "php": ">=5.6",
        "ext-dom": "*",
        "ext-xmlwriter": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "1.6.*",
        "phpunit/phpunit": "5.6.*",
        "satooshi/php-coveralls": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "HeitorFig\\WSBoletoSantander\\": "src/"
        }
    }
}
